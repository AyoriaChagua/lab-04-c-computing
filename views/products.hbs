<h1>Products</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Brand</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
        <tr>
        <th>{{id}}</th>
        <td>{{name}}</td>
        <td>{{brand}}</td>
        <td>{{price}}</td>
        </tr>
    {{/each}}
  </tbody>
</table>