<h1>clients</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Birthdate</th>
    </tr>
  </thead>
  <tbody>
    {{#each clients}}
        <tr>
        <th>{{id}}</th>
        <td>{{name}}</td>
        <td>{{lastname}}</td>
        <td>{{birthdate}}</td>
        </tr>
    {{/each}}
  </tbody>
</table>